{
    "$id": "#zedo",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Zedo adserver",
    "description": "https://github.com/ampproject/amphtml/blob/master/ads/zedo.md",
    "additionalProperties": false,
    "required": [
        "type",
        "super-id",
        "network",
        "placement-id",
        "channel",
        "publisher",
        "dim"
    ],
    "defaultSnippets": [
        {
            "label": "Zedo template",
            "description": "contains required keys",
            "body": {
                "type": "zedo",
                "super-id": "$1",
                "network": "$2",
                "placement-id": "$3",
                "channel": "$4",
                "publisher": "$5",
                "dim": "$6"
            }
        }
    ],
    "properties": {
        "type": {
            "const": "zedo"
        },
        "super-id": {
            "type": "string"
        },
        "network": {
            "type": "string"
        },
        "placement-id": {
            "type": "string"
        },
        "channel": {
            "type": "string"
        },
        "publisher": {
            "type": "string"
        },
        "dim": {
            "type": "string"
        },
        "charset": {
            "type": "string"
        },
        "callback": {
            "type": "string"
        },
        "renderer": {
            "type": "string"
        }
    }
} 