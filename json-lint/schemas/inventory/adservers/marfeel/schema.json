{
    "$id": "#marfeel",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Marfeel adserver",
    "description": "marfeelinventory.*.json defines the marfeel position DFP. We can use this adserver of the inventory to pass extra targetings and experimental parameters",

    "not": {
        "anyOf": [
            {
                "required": [
                    "type"
                ]
            },
            {
                "required": [
                    "slot"
                ]
            },
            {
                "required": [
                    "width"
                ]
            },
            {
                "required": [
                    "height"
                ]
            },
            {
                "required": [
                    "multi_size"
                ]
            },
            {
                "required": [
                    "multi_size_validation"
                ]
            },
            {
                "required": [
                    "force_safeframe"
                ]
            }
        ]
    }
  }
