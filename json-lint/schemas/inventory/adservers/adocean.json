{
    "$id": "#adocean",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "AdOcean adserver",
    "description": "https://github.com/ampproject/amphtml/blob/master/ads/adocean.md",
    "additionalProperties": false,
    "required": [
      "type",
      "ao-id",
      "ao-emitter"
    ],
    "defaultSnippets": [
      {
        "label": "AdOcean template",
        "description": "contains required keys",
        "body": {
          "type": "adocean",
          "ao-id": "$1",
          "ao-emitter": "$2"
        }
      }
    ],
    "properties": {
      "type": {
        "const": "adocean"
      },
      "ao-id": {
        "type": "string"
      },
      "ao-emitter": {
        "type": "string"
      },
      "ao-mode": {
          "type": "string"
      },
      "ao-preview": {
          "type": "string"
      },
      "ao-keys": {
        "type": "string"
      },
      "ao-vars": {
        "type": "string"
      },
      "ao-clusters": {
        "type": "string"
      },
      "width": {
        "$ref": "./types/dimension.json"
      },
      "height": {
        "$ref": "./types/dimension.json"
      },
      "heights": {
        "type": "string"
      }
    }
  }