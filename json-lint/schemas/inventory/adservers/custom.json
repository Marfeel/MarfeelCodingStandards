{
    "$id": "#custom",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Custom adserver",
    "description": "Only supported in touch",
    "additionalProperties": true,
    
    "required": [
      "type",
      "file"
    ],

    "defaultSnippets": [
      {
        "label": "Custom adServer template",
        "description": "contains required keys",
        "body": {
          "type": "custom",
          "file": "$1"
        }
      }
    ],

    "properties": {
      "type": {"const" : "custom"},
      "file": {
        "type": "string",
        "description": "name of the custom adServer"
      }
    }
  }