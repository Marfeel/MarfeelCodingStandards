{
    "$id": "#gumgum",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "GumGum adserver",
    "description": "https://github.com/ampproject/amphtml/blob/master/ads/gumgum.md",
    "additionalProperties": false,
    "required": [
        "type",
        "zone",
        "slot"
    ],
    "defaultSnippets": [
        {
            "label": "GumGum template",
            "description": "contains required keys",
            "body": {
                "type": "gumgum",
                "zone": "$1",
                "slot": "$2"
            }
        }
    ],
    "properties": {
        "type": {
            "const": "gumgum"
        },
        "zone": {
            "type": "string",
            "description": "GumGum Zone ID"
        },
        "slot": {
            "type": "string",
            "description": "GumGum Slot Ad ID"
        },
        "width": {
            "$ref": "./types/dimension.json"
        },
        "height": {
            "$ref": "./types/dimension.json"
        }
    }
}  