{
  "$id": "#iframeVideoAD",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "iframeVideo AD",
  "description": "AdServer Implemented for using embeds of videos for touch and amp",
  "additionalProperties": false,
  "required": [
    "type",
    "src"
  ],
  "defaultSnippets": [
    {
      "label": "iframeVideoAD template",
      "description": "contains required keys",
      "body": {
        "type": "iframeVideo",
        "src": "$1"
      }
    }
  ],
  "properties": {
    "type": {
      "const": "iframeVideo"
    },
    "src": {
      "type": "string",
      "description": "The src of the video embed"
    }
  }
}