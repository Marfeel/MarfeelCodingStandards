{
    "$id": "#adincube",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "adincube adserver",
    "description": "https://github.com/ampproject/amphtml/blob/master/ads/adincube.md",
    "additionalProperties": false,
    "required": [
        "type",
        "ad-type",
        "site-key"
    ],
    "defaultSnippets": [
        {
            "label": "adincube template",
            "description": "contains required keys",
            "body": {
                "type": "adincube",
                "ad-type": "$1",
                "site-key": "$2",
                "width": "$3",
                "height": "$4"
            }
        }
    ],
    "properties": {
        "type": {
            "const": "adincube"
        },
        "ad-type": {
            "type": "string"
        },
        "site-key": {
            "type": "string"
        },
        "width": {
            "$ref": "./types/dimension.json"
        },
        "height": {
            "$ref": "./types/dimension.json"
        }
    }
}