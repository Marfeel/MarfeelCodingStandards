{
    "$id": "#widgetEurWeb",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "WidgetEurweb adserver",
    "description": "AdServer Implemented For Press Widget mobile.eurweb.com",
    "additionalProperties": false,
    "required": [
        "type",
        "selector",
        "src",
        "height"
    ],
    "defaultSnippets": [
        {
        "label": "WidgetEurweb template",
        "description": "contains required keys",
        "body": {
            "type": "widget",
            "selector": "$1",
            "src": "$2",
            "height": "$3"
        }
    }
    ],
    "properties": {
        "type": {
            "const": "widget"
    },
        "selector": {
            "type": "string"
    },
        "src": {
            "type": "string"
    },
        "height": {
            "type": "string"
        }
    }
}