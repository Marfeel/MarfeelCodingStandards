{
  "$id": "#AdThrive",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "AdThrive adserver",
  "description": "Ad server not supported by amp. Only Web",
  "additionalProperties": false,
  "required": [
    "type",
    "id",
    "location",
    "sticky"
  ],
  "defaultSnippets": [
    {
      "label": "AdThrive template",
      "description": "contains required keys",
      "body": {
        "type": "adThrive",
        "id": "$1",
        "location": "$2",
        "sticky": "$3"
      }
    }
  ],
  "properties": {
    "type": {
      "const": "adThrive"
    },
    "id": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "sticky": {
      "type": "string"
    }
  }
}
