{
    "$id": "/properties/parsely",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Define Parsely as Metrics Provider",
    "title": "Parsely Metrics Provider",
    "type": "array",
    "items": {
        "additionalProperties": false,
        "properties": {
            "name": {
                "description": "Name of the metrics provider",
                "title": "Metrics Provider Name",
                "type": "string"
            },
            "apiKey": {
                "type": "string"
            }
        },
        "required": [
            "name",
            "apiKey"
        ]
    },
    "defaultSnippets": [
        {
            "body": [{
                "name": "$1",
                "apiKey": "$2"
            }]
        }
    ]
}