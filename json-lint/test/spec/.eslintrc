{
	"env": {
		"jasmine": true,
		"node": true
	},
	"globals": {
		"waitsForAndRuns": true,
		"readFixtures": true,
		"preloadFixtures": true,
		"loadFixtures": true,
		"setFixtures": true,
		"spyOnEvent": true
	},
	"rules": {
		//jasmine depends on nested callbacks to define suites and tests
		"max-nested-callbacks": "off"
	}
}
